<ul id='archives'>
{% for page in pages %}
    <li{% if page.filename in popular %} class='popular'{% endif %}>
    <time date='{{ page.date }}'>{{ page.date.strftime('%B %e %Y') }}</time>
    <a href='{{ page.url() }}'>{{ page.title|escape|decode }}</a>
    </li>
{% endfor %}
</ul>

