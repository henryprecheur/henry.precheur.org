<!DOCTYPE html>
<title>{% block title %}{{ title|escape|decode }}{% endblock %}</title>
{% block feed %}
<link rel="alternate" type="application/atom+xml" title="{{ title|escape|decode }} - Feed" href="{{ top_dir }}feed.atom">
{% endblock %}
{% block head %}
<link rel='stylesheet' href='{{ top_dir }}style.css'>
<!--[if lt IE 9]>
<script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->
{% endblock %}

<body>

<header>
<nav>
    <ul>
    <li>
        <form method='GET' action="http://www.google.com/search">
        <div>
        <input type="search" placeholder='Search...' name="q" maxlength="255" value="">
        <input type="hidden" name="sitesearch" value="henry.precheur.org">
        </div>
        </form>
    </li>
        <li><a href='{{ top_dir }}index.html'>Homepage</a></li>
    </ul>
</nav>
</header>

<article>
{% block content %}{% endblock %}
</article>

{% block navigation %}{% endblock %}

<footer>&copy; 2007-2011
<a href='{{ top_dir }}about.html'>Henry Pr&ecirc;cheur</a></footer>

<script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-20945988-1']);
  _gaq.push(['_trackPageview']);
  _gaq.push(['_trackPageLoadTime']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script>

</body>
