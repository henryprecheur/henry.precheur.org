{% extends 'base.html.tmpl' %}

{% block content %}
<h1>Hi I'm <a href='/about'>Henry</a></h1>

<div id=posts>
{% for page in pages %}
    <div {% if page.filename in popular %} class='popular'{% endif %}>
    <time datetime='{{ page.date|rfc3339 }}'>{{ page.date.strftime('%B %e %Y') }}</time
    ><span><a href='{{ page.url() }}'>{{ page.title|escape|decode }}</a></span>
    </div>
{% endfor %}
</div>

</nav>


{% endblock %}

{% block navigation %}
{{ super() }}
<h3>Older posts</h3>

{% set pages = pages[1:] %}
{% include 'pages_summary.html.tmpl' %}
{% endblock %}

